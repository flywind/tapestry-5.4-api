<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter">
<h3>${message:compoment-desc}</h3>
<h4>FDatagrid params:</h4>
<table class="tapestry-tab" border="0">
  <thead>
    <tr>
      <th>${message:grid-name}</th>
      <th>${message:grid-type}</th>
      <th>${message:grid-symbol}</th>
      <th>${message:grid-default}</th>
      <th>${message:grid-default-prefix}</th>
      <th>${message:grid-since}</th>
      <th>${message:grid-desc}</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>id</td>
      <td>String</td>
      <td>Required,Not Null</td>
      <td></td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc1}</td>
    </tr>
    <tr>
      <td>params</td>
      <td>JSONObject</td>
      <td></td>
      <td>{'method':'POST','width':'auto','fit':true,'fitColumns':false,'pagination':true,'rownumbers':true,'loadMsg':messages.get("loading-data"),'remoteSort':false,'pageSize':10,'striped':true,'nowrap':true,'singleSelect':true,'language':'zh-cn','fixLayout':false}</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc2}</td>
    </tr>
    <tr>
      <td>singleSelect</td>
      <td>boolean</td>
      <td></td>
      <td>true</td>
      <td></td>
      <td>1.0</td>
      <td>${message:desc3}</td>
    </tr>
    <tr>
      <td>source</td>
      <td>list</td>
      <td>Required,Not Null</td>
      <td></td>
      <td></td>
      <td>1.0</td>
      <td>${message:desc4}</td>
    </tr>
  </tbody>
  <tfoot><tr><td colspan="7">${message:grid-info}</td></tr></tfoot>
</table>
<h4>FDatagridBtn params:</h4>
<table class="tapestry-tab" border="0">
  <thead>
    <tr>
      <th>${message:grid-name}</th>
      <th>${message:grid-type}</th>
      <th>${message:grid-symbol}</th>
      <th>${message:grid-default}</th>
      <th>${message:grid-default-prefix}</th>
      <th>${message:grid-since}</th>
      <th>${message:grid-desc}</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>clientId</td>
      <td>String</td>
      <td>Required,Not Null</td>
      <td></td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc5}</td>
    </tr>
    <tr>
      <td>text</td>
      <td>String</td>
      <td>Required,Not Null</td>
      <td></td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc6}</td>
    </tr>
    <tr>
      <td>url</td>
      <td>String</td>
      <td>Required,Not Null</td>
      <td></td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc7}</td>
    </tr>
    <tr>
      <td>gridId</td>
      <td>String</td>
      <td>Required,Not Null</td>
      <td></td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc8}</td>
    </tr>
    <tr>
      <td>cls</td>
      <td>String</td>
      <td></td>
      <td>btn</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc9}</td>
    </tr>
    <tr>
      <td>itemcls</td>
      <td>String</td>
      <td></td>
      <td>fa</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc10}</td>
    </tr>
    <tr>
      <td>clientEvent</td>
      <td>String</td>
      <td></td>
      <td>click</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc11}</td>
    </tr>
    <tr>
      <td>linkType</td>
      <td>String</td>
      <td></td>
      <td>event</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc12}</td>
    </tr>
    <tr>
      <td>zone</td>
      <td>String</td>
      <td></td>
      <td></td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc13}</td>
    </tr>
    <tr>
      <td>alertMsg</td>
      <td>String</td>
      <td></td>
      <td>请选择至少一条记录(中文),Please select at least one record(英文)</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc14}</td>
    </tr>
    <tr>
      <td>alertTitle</td>
      <td>String</td>
      <td></td>
      <td>温馨提示(中文),Tips(英文)</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc15}</td>
    </tr>
    <tr>
      <td>confirmTitle</td>
      <td>String</td>
      <td></td>
      <td>确认信息(中文),Confirmation(英文)</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc16}</td>
    </tr>
    <tr>
      <td>confirmMsg</td>
      <td>String</td>
      <td></td>
      <td>您确定吗?(中文),Are you sure?(英文)</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc17}</td>
    </tr>
    <tr>
      <td>submitMsg</td>
      <td>String</td>
      <td></td>
      <td>确认(中文),Ok(英文)</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc18}</td>
    </tr>
    <tr>
      <td>cancelMsg</td>
      <td>String</td>
      <td></td>
      <td>取消(中文),Cancel(英文)</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc19}</td>
    </tr>
  </tbody>
  <tfoot><tr><td colspan="7">${message:grid-info}</td></tr></tfoot>
</table>

<form class="form-inline" t:type="form" role="form" t:zone="zoneOne" t:autofocus="false">
			<div class="form-group">
				<label class="control-label">User name<span class="mr5">:</span></label>
				<input class="iw120 form-control" type="text" placeholder="Input something" t:type="textfield" t:id="userName"/>
			</div>
			<div class="form-group">
				<button class="btn btn-block btn-info btn-flat">Search</button>
			</div>
</form>
<div class="btn-box">
	         <div class="btn-group mr10">
	           <t:widgets.fdatagridbtn t:clientId="edit" linkType="page" text="Edit data" url="FDatagridModeTest" gridId="newContracts"/>
	         </div>
	         <div class="btn-group mr10">
	           <t:widgets.fdatagridbtn t:clientId="edit5" linkType="page" paramsMode="true" text="Edit data again" url="FDatagridTest4" gridId="newContracts"/>
	         </div>
	         <div class="btn-group mr10">
	         	<t:widgets.fdatagridbtn t:clientId="delete" zone="zoneOne"  text="Delete data" url="${delUrl}" alertMsg="No data selected" gridId="newContracts" />
	         </div>
</div>
<t:zone t:id="zoneOne" id="zoneOne" style="height:500px">
<table t:type="widgets/fdatagrid" t:id="newContracts" t:source="examples" t:params="{'language':'en'}" t:pageSize="20">
	<thead>
		<tr>
			<th data-options="field:'id',width:80,title:'ID',sortable:true"></th>
			<th data-options='field:"creater",width:300,title:"Creater name",
							  formatter:function(value,row){
							  	if(row.creater == "kitty"){
							  		
							  		return row.creater+" &lt;a&gt;htttt&lt;/a&gt;";
							  	}else{
							  		return row.creater+" world";
							  	}
							  }'></th>
		    <th data-options="field:'customerCode',width:80,title:'Customer code'"></th>
		    <th data-options="field:'userName',width:80,title:'${message:userName-label}'"></th>
		    <th data-options="field:'createTime',width:150,title:'Create time'"></th>
		    <th data-options="field:'lastUpdateTime',width:150,title:'Updata time'"></th>
		    <th data-options="field:'lastUpdatePerson',width:80,title:'Updata person'"></th>
		</tr>
	</thead>
</table>
</t:zone>
<div class="btn-box">
	<div class="btn-group mr10"><t:widgets.fdatagridbtn t:clientId="editT" linkType="page"  text="Delete go ..." url="FDatagridTest2" gridId="newContracts2"/></div>
	<div class="btn-group mr10"><t:widgets.fdatagridbtn t:clientId="deleteT" zone="zoneTwo"  text="Delete again" url="${deleteUrl}" alertMsg="No data selected" gridId="newContracts2" /></div>
</div>
<t:zone t:id="zoneTwo" id="zoneTwo" style="height:500px;">
<table t:type="widgets/fdatagrid" t:id="newContracts2" t:source="examples">
	<thead>
		<tr>
			<th data-options="field:'userName',width:80,title:'User name',sortable:true"></th>
			<th data-options="field:'creater',width:80,title:'Create name'"></th>
		</tr>
	</thead>
</table>
</t:zone>
</html>
