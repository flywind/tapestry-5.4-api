<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter">
<h3>${message:compoment-desc}</h3>
<table class="tapestry-tab" border="0">
  <thead>
    <tr>
      <th>${message:grid-name}</th>
      <th>${message:grid-type}</th>
      <th>${message:grid-symbol}</th>
      <th>${message:grid-default}</th>
      <th>${message:grid-default-prefix}</th>
      <th>${message:grid-since}</th>
      <th>${message:grid-desc}</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>message</td>
      <td>String</td>
      <td></td>
      <td>您确定吗?(中文),Are you sure?(英文)</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc1}</td>
    </tr>
    <tr>
      <td>title</td>
      <td>String</td>
      <td></td>
      <td>确认信息(中文),Confirmation(英文)</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc2}</td>
    </tr>
    <tr>
      <td>validationMsg</td>
      <td>String</td>
      <td></td>
      <td>确定(中文),ok(英文)</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc3}</td>
    </tr>
    <tr>
      <td>cancelMsg</td>
      <td>String</td>
      <td></td>
      <td>取消(中文),Cancel(英文)</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc4}</td>
    </tr>
    <tr>
      <td>padding</td>
      <td>int</td>
      <td></td>
      <td>180</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc5}</td>
    </tr>
    <tr>
      <td>lock</td>
      <td>boolean</td>
      <td></td>
      <td>true</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc6}</td>
    </tr> 
    <tr>
      <td>useDefaultConfirm</td>
      <td>boolean</td>
      <td></td>
      <td>false</td>
      <td>literal</td>
      <td>1.0</td>
      <td>${message:desc7}</td>
    </tr>
  </tbody>
  <tfoot><tr><td colspan="7">${message:grid-info}</td></tr></tfoot>
</table>
<t:actionlink t:id="del" t:zone="meZone" t:context="1" t:mixins="widgets/FConfirm" FConfirm.padding="20px 150px" FConfirm.lock="true" FConfirm.useDefaultConfirm="false">delete</t:actionlink>
<t:zone t:id="meZone" id="meZone">${count}</t:zone>
</html>
